{
  "isDev": false,

  "watchers": {
    "logOptions": {
      "level": "info",
      "console": {
        "color": false
      },
      "app": {
        "file": {
          "filename": "./logs/:type.log",
          "format": ":time :level :data",
          "timestamp": "HH:mm:ss"
        }
      }
    }
  },

  "postgres": "$POSTGRES_URL"
}
